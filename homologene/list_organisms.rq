#!/usr/bin/env spang2
# @title List organisms included in HomoloGene
# @endpoint ortho
# @option -a

PREFIX orth: <http://purl.org/net/orth#>
PREFIX homologene: <https://ncbi.nlm.nih.gov/homologene/>

SELECT DISTINCT ?taxid ?label
WHERE {
  GRAPH homologene: {
    ?s a orth:Gene .
    ?s orth:taxon ?taxid .
  }
  ?taxid rdfs:label ?label .
}
ORDER BY ?label
